{
  "name": "blogspot",
  "version": "0.0.1",
  "description": "Create your own blog using blogspot.",
  "main": "server.js",
  "scripts": {
    "test": "npm run test",
    "build": "babel src -d dist",
    "start": "node --harmony dist/server",
    "watch": "babel -w src -d dist",
    "watch-d": "babel -w src -d dist & nodemon --harmony dist/server",
    "drop-db": "node ./dist/tools/drop-database.js",
    "sync-db": "node ./dist/tools/sync-database.js",
    "seed-db": "node ./dist/tools/seed-database.js",
    "admin-db": "node ./dist/tools/admin-database.js",
    "engines": "node ./dist/engines.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Davlis/blogspot-api.git"
  },
  "keywords": [
    "blogspot",
    "blog"
  ],
  "author": "Dawid Liszka",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Davlis/blogspot-api/issues"
  },
  "homepage": "https://github.com/Davlis/blogspot-api#readme",
  "babel": {
    "plugins": [
      "transform-es2015-modules-commonjs"
    ]
  },
  "dependencies": {
    "@sendgrid/mail": "^6.2.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "body-parser": "^1.18.2",
    "cors": "^2.8.4",
    "crypto": "^1.0.1",
    "dotenv": "^4.0.0",
    "express": "^4.16.2",
    "jsonwebtoken": "^8.1.0",
    "morgan": "^1.9.0",
    "pg": "^6.4.2",
    "pg-hstore": "^2.3.2",
    "pug": "^2.0.0-rc.4",
    "sequelize": "^4.13.15"
  },
  "devDependencies": {
    "nodemon": "^1.12.1"
  }
}
